{
  "name": "ah-frontend-dojo",
  "version": "1.0.0",
  "description": "Authors Haven",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --open --hot --mode development",
    "start": "node server.js",
    "build": "webpack --mode production",
    "lint": "eslint",
    "stylelint": "stylelint '**/*.scss'; exit 0",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "npm": "6.4.1",
    "node": "10.15.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "ignore": [],
    "linters": {
      "*.{js,jsx}": [
        "eslint src/ --fix",
        "git add"
      ],
      "*.{scss,css}": [
        "stylelint '**/*.scss'",
        "git add"
      ]
    }
  },
  "lint-committed": {
    "ignore": [],
    "linters": {
      "*.{js,jsx}": [
        "eslint src/",
        "npm run lint",
        "npm run test"
      ],
      "*.{scss}": [
        "npm run stylelint"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/ah-frontend-dojo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela/ah-frontend-dojo/issues"
  },
  "homepage": "https://github.com/andela/ah-frontend-dojo#readme",
  "dependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "express": "^4.17.1",
    "jest-css-modules-transform": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "serve": "^11.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@commitlint/travis-cli": "^7.6.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-css-modules-transform": "^2.2.0",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
